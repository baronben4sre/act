mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'update transactions set category_id=88 where category_id=72 and libel like "%amazon%" or libel like "AMZ%"'

# Cash
mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'update transactions set category_id=102 where category_id=72 and libel like "%ATM WITHDRAWA%"'
mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'update transactions set category_id=102 where category_id=72 and libel like "%ZEL %"'
mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'update transactions set category_id=102 where category_id=72 and libel like "Check Paid%" and (amount = -1000 or amount = -500 or amount = -350)' # Check for Loic
mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'update transactions set category_id=102 where category_id=72 and libel like "% CASH APP%"'

mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'update transactions set category_id=1  where libel like "%Amazon Prime%" and amount < - 100 order by operation'


echo "Unclassify Operations:"
mysql $MYSQL_DATABASE -u$MYSQL_USER -p$MYSQL_PASSWORD -e 'select * from transactions where category_id=72 and libel not like "%amazon%" and libel not like "AMZ%"'
nohup sh -c 'sleep 3600 && $ACTBASE/bin/act-summarization' >&$ACTBASE/log/lastSummarization.log &
echo "$(date) INFO In one hour, the summarization will start in the background. Log can be found there: $ACTBASE/log/lastSummarization.log" | tee -a $ACTBASE/log/act.log
